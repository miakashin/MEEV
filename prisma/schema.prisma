datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Applicant {
  id                    String    @id @default(uuid())
  firstName             String
  lastName              String
  educationalAttainment String
  schoolName            String
  phoneNumber           String?
  address               String?
  interview             DateTime?
  createdAt             DateTime  @default(now())
  updatedAt             DateTime  @updatedAt
}