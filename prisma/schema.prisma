  datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
 model Applicant {
     id                    String    @id @default(uuid())
     firstName             String
     lastName              String
     educationalAttainment String
     schoolName            String
     phoneNumber           String?
     address               String?
     interview             DateTime?
     createdAt             DateTime  @default(now())
     updatedAt             DateTime  @updatedAt
   }